{
  "version": 2,
  "builds": [
    {
      "src": "api/convert.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/convert",
      "methods": ["GET"],
      "dest": "api/convert.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/convert.js",
      "status": 404
    }
  ]
}
